{
    "include": [
        "*.ts",
        "*.js",
        "*.tsx",
    ],
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "Bundler",
        "sourceMap": true,
        "outDir": "dist/js",
        "allowJs": true,
        "types": [
            "vite/client"
        ],
        "jsx": "react-jsx",
        "jsxImportSource": "preact",
    }
}